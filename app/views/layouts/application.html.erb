<!DOCTYPE html>
<html>
<head>
  <title>TestApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <%= render 'layouts/header' %>

  <div class='row'>
    <div class='col-xs-3'>
      <%= render 'layouts/aside' %>
    </div>
    <div class='col-xs-9'>
      <%= yield %>

    </div>
  </div>


  <%= render 'layouts/footer' %>

</div>
</body>
</html>
